<h1>Products#index</h1>
<p>Find me in app/views/products/index.html.erb</p>

<% @products.each_with_index do |item, index| %>
   <div> 
      <span><%= item.id %></span>  
      <span><%= item.title %></span>  
      <span><%= item.description %></span>  
      <span><%= item.price %>$</span>  
   </div>
<% end %>

<%= will_paginate @products %>